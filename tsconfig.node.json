{
  "compilerOptions": {
    "strict": true,
    "rootDir": ".",
    "outDir": "./dist/backend",
    "types": ["node", "express", "dotenv"],
    "moduleResolution": "bundler",
    "moduleDetection": "force",
    "target": "ES2023",
    "declaration": true,
    "composite": true,
    // "noEmit": true,
    "lib": ["ES2023"],
    "module": "ESNext"
  },
  "include": ["./extensions/**/**/*.ts", "./server/**/*ts"],
  "exclude": ["dist", "node_modules"]
}
