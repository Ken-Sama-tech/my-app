{
  "name": "my-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "copy-html": "copyfiles -u 1 ./server/public/* dist/backend/server",
    "dev": "concurrently \"--env-file=.env\" \"vite --host\" \"nodemon ./dist/backend/server/server.js\" \"tsc -b -w\" ",
    "build": "concurrently \"rimraf dist\" \"tsc -b\"  \"vite build && npm run copy-html\"",
    "lint": "eslint .",
    "preview": "concurrently \"vite preview\" \"--env-file=.env\"  \"nodemon ./dist/backend/server/server.js\""
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.12",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "fuse.js": "^7.1.0",
    "lucide-react": "^0.540.0",
    "mongoose": "^8.20.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/vite": "^4.1.18",
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.6",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^24.10.4",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "concurrently": "^9.2.1",
    "copyfiles": "^2.4.1",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.26",
    "express": "^5.2.1",
    "globals": "^16.5.0",
    "i": "^0.3.7",
    "nodemon": "^3.1.11",
    "npm": "^11.7.0",
    "rimraf": "^6.1.2",
    "tailwindcss": "^4.1.18",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1",
    "vite": "^7.3.0"
  },
  "prettier": {
    "tabWidth": 2
  }
}
