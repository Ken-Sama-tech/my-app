  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "copy-html": "copyfiles -u 1 ./server/public/* dist/backend/server",
    "dev": "concurrently \"tsc -w\" \"vite\" \"nodemon ./dist/backend/server/server.js\"",
